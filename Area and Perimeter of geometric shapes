#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    float r,s,l,b,p,a;
    float const pi=3.1416;
    
    scanf("%f\n%f\n%f%f",&r,&s,&l,&b);
    
    a=pi*r*r;
    p=2*pi*r;
    printf("Circle: %.2f %.2f\n",a,p);
    
    a=s*s;
    p=4*s;
    printf("Square: %.2f %.2f\n",a,p);
    
    a=l*b;
    p=2*(l+b);
    printf("Rectangle: %.2f %.2f\n",a,p);
    
    
    return 0;
}
