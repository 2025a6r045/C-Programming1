#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

 
    int a,b,c,d,e;
    float per;
    scanf("%d%d%d%d%d",&a,&b,&c,&d,&e);
    per=((a+b+c+d+e)/500.0)*100.0;
    printf("Percentage = %.2f\n",per);
    if (per>=90){
        printf("Grade = A");
    }
    else if (per<90 && per>=80){
        printf("Grade = B");
    }
    else if (per<80 && per>=70){
        printf("Grade = C");
    }
    else if (per<70 && per>=60){
        printf("Grade = D");
    }
    else if (per<60 && per>=40){
        printf("Grade = E");
    }
    else{
        printf("Grade = F");
    }
    return 0;
}
