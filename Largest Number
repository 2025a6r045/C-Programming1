Largest Number-1

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int a,b;
    scanf("%d%d",&a,&b);
    (a>b)?printf("%d",a):printf("%d",b);
    return 0;
}


Largest Number-2


#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
  
    int a,b,c;
    
    scanf("%d%d%d",&a,&b,&c);
    
    if (a>=b && a>=c){
        printf("%d",a);
    }
    else if (b>=a && b>=c){
        printf("%d",b);
    }
    else{
        printf("%d",c);
    }
   
    return 0;
}




